---
type News = {
  order: number;
  category: string;
  url: string;
  image: string;
  title: string;
  summary: string
}

type Reverse = {
  rever: string;
  first: string;
  last: string;
}


export interface Props {
  news: News
  reverse: Reverse
}

const {news, reverse} = Astro.props
const {order,category, url, image, title, summary} = news
console.log(reverse)
---

<!-- ARTICLE PHOTO -->

      <td class="column column-1 mn-column1-article" class={reverse.first} width="60%">
        <table class="image_block block-1" width="100%">
          <tr>
            <td class="pad mn-pad-article-image">
              <div class="alignment mn-alignment-article-image" align="center">
                <div class="mn-div-article-image">
                  <a href={url} tabindex="-1" rel="noreferrer noopener">
                    <img src={image}
                      target="_blank"
                      class="mn-img-article-image" width="266.666666666663"
                      alt={title}>
                  </a>
                </div>
              </div>
            </td>
          </tr>
        </table>
      </td>
