---
import Main311  from './Main311.astro';
import Main312 from './Main312.astro';
import type { News } from '../services/interfaces/newsletter.d.ts'
import { calculeClasesReverse } from '../services/getnewsletters'
export interface Props {
  news: News;
}

const {news } = Astro.props

const reverse = calculeClasesReverse(news)
---


<!-- ARTICLE -->
{

<table class="row row-3" align="center" width="100%">
  <tbody>
    <tr>
      <td>
        
        <table class="row-content stack mn-row-content-article" align="center" width="800" >
          <tbody>
            <tr class={reverse.rever}>
                {   reverse.rever == 'reverse'
                    ? <Main311  news={news} reverse={reverse}/>
                      <Main312  news={news} reverse={reverse}/>
                    :  
                      <Main312  news={news} reverse={reverse}/>
                      <Main311  news={news} reverse={reverse}/>
                }
            </tr>
          </tbody>
        </table>
      </td> 
    </tr> 
  </tbody>
</table>


}
<!-- End of ARTICLE -->